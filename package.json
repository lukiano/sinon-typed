{
  "name": "sinon-typed",
  "version": "0.1.0",
  "description": "Nicer stubbing in a TypeScript environment",
  "keywords": [
    "sinon",
    "typescript",
    "mocking",
    "stubbing"
  ],
  "main": "out/index.js",
  "scripts": {
    "build": "npm run compile && npm run lint && npm run fmt",
    "compile": "tsc",
    "pretest": "npm run build",
    "test": "npm run test-only",
    "test-ts": "mocha --require ts-node/register `find ./lib -name '*-spec.ts' -print`",
    "test-only": "mocha `find ./out -name '*-spec.js' -print`",
    "fmt": "tsfmt -r",
    "lint": "tslint --project './tsconfig.json' -c tslint.json",
    "precoverage": "npm run build",
    "coverage": "node node_modules/istanbul/lib/cli.js cover -e .js -x '*-spec.js' _mocha -- `find ./out -name '*-spec.js' -print`",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls"
  },
  "author": "Luciano Leggieri",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/lukiano/sinon-typed/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lukiano/sinon-typed.git"
  },
  "peerDependencies": {
    "@types/sinon": ">= 5",
    "sinon": ">= 5",
    "typescript": "^3.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/mocha": "^5.2.5",
    "@types/sinon": "^5.0.5",
    "chai": "^4.2.0",
    "coveralls": "^3.0.1",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "^5.2.0",
    "sinon": "^6.3.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-consistent-codestyle": "^1.13.3",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-microsoft-contrib": "^5.2.1",
    "typescript": "^3.1.2",
    "typescript-formatter": "^7.2.2"
  },
  "typings": "./out/index.d.ts"
}
